{
    "name": "artisanry/social-share",
    "description": "",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-social-share",
        "Laravel Social Share"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\SocialShare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\SocialShare\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\SocialShare\\SocialShareServiceProvider"
            ],
            "aliases": {
                "SocialShare": "Artisanry\\SocialShare\\Facades\\SocialShare"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
